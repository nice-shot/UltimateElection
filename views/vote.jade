extends ./layout.jade

block content
	div.row
		div#note(class=[
				"col-xs-10",
				"col-sm-3",
				"col-md-3",
				"col-lg-2",
				"center-block"
			])
			p#party.text-center.noselect= party

	div.row
		h2
			span= "Score: "
			span#score

		h3
			span#users
			span= " connected users"

		pre#stats
		button#statsBtn Refresh

block jsCode
	script(src="js/vote.js")
